{
	"name": "Musidex YT Fetch",
	"version": "0.0.1",
	"description": "Upload song to playlist directly from youtube",
	"homepage_url": "https://github.com/Uriopass/Musidex",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage",
		"*://*.youtube.com/watch*"
	],
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"content_scripts": [
		{
			"matches": ["*://*.youtube.com/watch*"],
			"js": ["yt.js"]
		}
	],
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	}
}
